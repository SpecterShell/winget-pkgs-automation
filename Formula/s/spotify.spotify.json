{
  "$schema": "https://github.com/vedantmgoyal2009/winget-pkgs-automation/raw/main/schema.json",
  "Identifier": "Spotify.Spotify",
  "Update": [
    {
      "InvokeType": "WebRequest",
      "Uri": "https://download.scdn.co/SpotifyFullSetup.exe",
      "Method": "Head",
      "Headers": {},
      "Body": "",
      "UserAgent": ""
    }
  ],
  "PostResponseScript": "$Response = $Response.BaseResponse.RequestMessage.RequestUri.OriginalString",
  "VersionRegex": "[0-9.]+",
  "InstallerRegex": ".(exe|msi|msix|appx)(bundle){0,1}$",
  "PreviousVersion": "",
  "ManifestFields": {
    "PackageVersion": "(winget-pkgs-automation get-installer-info --json \"$($Response)\" | ConvertFrom-Json).PackageVersion",
    "InstallerUrls": "$Response"
  },
  "AdditionalInfo": {},
  "PostUpgradeScript": "",
  "SkipPRCheck": false,
  "SkipPackage": false
}
