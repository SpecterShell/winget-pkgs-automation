{
  "$schema": "https://github.com/vedantmgoyal2009/winget-pkgs-automation/raw/main/schema.json",
  "Identifier": "Youdao.YoudaoDict",
  "Update": [
    {
      "InvokeType": "WebRequest",
      "Uri": "http://cidian.youdao.com/download/YoudaoDict.exe",
      "Method": "Head",
      "Headers": {},
      "Body": "",
      "UserAgent": ""
    }
  ],
  "PostResponseScript": "$Response = $Response.BaseResponse.RequestMessage.RequestUri.OriginalString",
  "VersionRegex": "[0-9.]+",
  "InstallerRegex": ".(exe|msi|msix|appx)(bundle){0,1}$",
  "PreviousVersion": "",
  "ManifestFields": {
    "InstallerUrls": "$Response.Replace('http://','https://')",
    "PackageVersion": "(winget-pkgs-automation get-installer-info --json \"$($Update.InstallerUrls)\" | ConvertFrom-Json).PackageVersion"
  },
  "AdditionalInfo": {},
  "PostUpgradeScript": "",
  "SkipPRCheck": false,
  "SkipPackage": false
}
